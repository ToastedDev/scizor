{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    "src",
    "astro.config.mjs",
    ".eslintrc.cjs",
    "**/*.js",
    "**/*.cjs",
    "**/*.mjs",
    "./tailwind.config.mjs",
    "../packages/scizor/src"
  ]
}
